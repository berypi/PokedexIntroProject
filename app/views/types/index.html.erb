<p id="notice"><%= notice %></p>

<h1>Types</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @types.each do |type| %>
      <tr>
        <td><%= type.poke_type %></td>
        <td><%= link_to 'Show', type %></td>
      </tr>
    <% end %>
  </tbody>
</table>

