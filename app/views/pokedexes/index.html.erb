<p id="notice"><%= notice %></p>

<h1>Pokedexes</h1>

<table>
  <thead>
    <tr>
      <th>Pokedex name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pokedexes.each do |pokedex| %>
      <tr>
        <td><%= pokedex.pokedex_name %></td>
        <td><%= link_to 'Show', pokedex %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
